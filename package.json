{
  "name": "express-server-timing-middleware",
  "version": "2.0.5-0",
  "description": "Express middleware to track server time spent on operations and write Server-Timing headers",
  "types": "./dist/cjs/types/index.d.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.22.1",
    "@babel/preset-env": "^7.22.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/node": "^16.18.34",
    "@types/on-headers": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-jest": "^29.5.0",
    "codecov": "^3.8.3",
    "eslint": "^8.41.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.27.5",
    "jest": "^29.5.0",
    "on-headers": "^1.0.2",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rimraf ./dist/*",
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json && mv ./dist/esm/index.js ./dist/esm/index.mjs",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "test": "jest --config ./config/jest/jest.config.js",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "test:ci": "npm run test:coverage && codecov",
    "prepack": "npm run build",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dashukin/express-server-timing-middleware.git"
  },
  "keywords": [
    "Express",
    "Server-Timing",
    "Performance"
  ],
  "author": "Vasili Molakhau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dashukin/express-server-timing-middleware/issues"
  },
  "homepage": "https://github.com/dashukin/express-server-timing-middleware#readme"
}
